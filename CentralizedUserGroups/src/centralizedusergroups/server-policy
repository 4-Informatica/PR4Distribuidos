grant { 
    permission java.net.SocketPermission "127.0.0.1:*", "connect,resolve,listen";
    permission java.net.SocketPermission "127.0.0.1:*", "accept"; 
    permission java.net.SocketPermission "127.0.0.2:*", "connect,resolve,listen"; 
    permission java.net.SocketPermission "127.0.0.2:*", "accept";
};